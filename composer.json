{
  "name": "mwguerra/crud-artisan",
  "description": "A Laravel package to generate CRUD controllers and Vue components for Inertia.js",
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "MWGuerra\\CrudArtisan\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MWGuerra\\CrudArtisan\\CrudArtisanServiceProvider"
      ]
    }
  }
}
